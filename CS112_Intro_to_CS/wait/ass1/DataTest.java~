public class DataTest
{

    public static void main(String[] args)
    { 
        
	// create an Account object and assign it to myAccount
	Data date = new Data(9,11,2001); //using convenience constructor with three parameterized values 
	System.out.printf("The initial date is: %s", date.displayDate());
	date.setMonth(10); // put the month value in date
	date.setDay(17); // put the day value in date
	date.setYear(1989);
		
	// display the name stored in object myAccount
	System.out.printf("Date with new values is: %s%n",
			  date.displayDate());
    } 
} // end class DataTest