#include <iostream>
using namespace std;

int main()
{
  cout << "The Planck constant has\n";
  cout << " dimensions of physical action;\n"
  cout << "these are the same as those of angular momentum,"<< endl;
  cout << "i.e., energy multiplied by time, "<< endl;
  cout << "or momentum multiplied by distance. "<< endl;
  return 0;
}
